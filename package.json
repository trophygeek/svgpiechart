{
  "name": "svgpiechart",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@trussworks/react-uswds": "^6.0.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.30",
    "@types/react-dom": "^18.2.14",
    "dompurify": "^3.0.6",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "compile_scss_dev": "sass --load-path=./node_modules/@uswds/uswds/packages/ --embed-source-map --quiet-deps src/styles/global.scss:src/content/generated/global.out.css",
    "watch_scss": "yarn compile_scss_dev && sass --load-path=./node_modules/@uswds/uswds/packages/ --embed-source-map --quiet-deps -w src/styles/global.scss:src/content/generated/global.out.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/dompurify": "^3.0.4",
    "react-scripts": "^5.0.1",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "shell-quote": "^1.7.3",
    "sockjs": "^0.3.20",
    "terser": "^5.24.0",
    "tough-cookie": "^4.1.3",
    "yargs-parser": "^v21.1.1"
  }
